{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".editorconfig",
    ".eslintrc.js",
    ".node-version",
    ".npmrc",
    ".prettierignore",
    "angular.json",
    "dfx.json",
    "prettier.config.cjs",
    "tsconfig.json",
    ".env"
  ],
  "tasks": {
    "build": {
      "outputs": ["dist/**", "build/**", ".docusaurus/**", "loader/**", "www"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "start": {
      "outputs": [],
      "dependsOn": ["^build"]
    }
  }
}
