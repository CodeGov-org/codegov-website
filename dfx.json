{
  "dfx": "0.15.2",
  "output_env_file": ".env",
  "version": 1,
  "canisters": {
    "backend": {
      "type": "rust",
      "gzip": true,
      "optimize": "cycles",
      "package": "backend_impl",
      "candid": "src/backend/api/backend.did",
      "declarations": {
        "bindings": ["js", "ts", "did"],
        "output": "./dist"
      }
    },
    "frontend": {
      "dependencies": ["backend"],
      "frontend": {
        "entrypoint": "src/frontend/dist/browser/index.html"
      },
      "source": ["src/frontend/dist/browser"],
      "type": "assets",
      "build": ["pnpm -F frontend... build"]
    },
    "marketing": {
      "type": "assets",
      "gzip": true,
      "optimize": "cycles",
      "frontend": {
        "entrypoint": "src/marketing/dist/index.html"
      },
      "source": ["src/marketing/dist"],
      "build": ["pnpm -F marketing... build"]
    },
    "docs": {
      "type": "assets",
      "gzip": true,
      "optimize": "cycles",
      "frontend": {
        "entrypoint": "src/docs/build/index.html"
      },
      "source": ["src/docs/build"],
      "build": ["pnpm -F docs... build"]
    }
  }
}
